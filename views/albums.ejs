<h2 class="result-title">Albums for: <%- allAlbums[0].artists[0].name %></h2>
<div class="result-ctn">
  <% allAlbums.forEach(album => { %>
  <div class="card">
    <% if (typeof album !== 'undefined') { %>
    <img src="<%- album.images[0].url %> " alt="" />
    <% } %>
    <h2><%- album.name %></h2>
    <a href="/albums/tracks/<%- album.id %> ">View tracks</a>
  </div>
  <% }) %>
</div>
